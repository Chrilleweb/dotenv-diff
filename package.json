{
  "name": "dotenv-diff",
  "version": "1.6.2",
  "type": "module",
  "description": "A small CLI and library to find differences between .env and .env.example files.",
  "bin": {
    "dotenv-diff": "dist/bin/dotenv-diff.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "vitest --watch",
    "test": "vitest",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/*.ts\"",
    "start": "node dist/bin/dotenv-diff.js"
  },
  "author": "Chrilleweb",
  "license": "MIT",
  "keywords": [
    "dotenv",
    "env",
    "dotenv-diff",
    "cli",
    "compare",
    "environment variables",
    "dotenv validator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Chrilleweb/dotenv-diff.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "typescript": "^5.2.0",
    "vitest": "^3.2.0"
  }
}
